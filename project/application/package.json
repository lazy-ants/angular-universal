{
    "name": "angular-universal",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng run angular-universal:dev-serve--default --open",
        "start:ssr": "ng run angular-universal:dev-serve--ssr --open",
        "build:ssr": "ng run angular-universal:build-browser--silent:production && ng run angular-universal:build-server--silent:production",
        "serve:ssr": "rm -rf dist && cp -r tmp dist && npm run pm2:stop && npm run pm2:start",
        "pm2:start": "pm2 start dist/server/main.js --name angular-universal --max-restarts 5",
        "pm2:stop": "pm2 delete -s angular-universal || :",
        "format": "npm run format-ts && npm run format-scss && npm run format-html",
        "format-ts": "prettier --write \"**/*.ts\" --config \".prettierrc-ts.json\"",
        "format-scss": "prettier --write \"**/*.scss\" --config \".prettierrc-scss.json\"",
        "format-html": "prettier --write \"**/*.html\" --config \".prettierrc-html.json\""
    },
    "private": true,
    "husky": {
        "hooks": {
            "pre-commit": "npm run format"
        }
    },
    "dependencies": {
        "@angular/animations": "9.1.11",
        "@angular/common": "9.1.11",
        "@angular/compiler": "9.1.11",
        "@angular/core": "9.1.11",
        "@angular/forms": "9.1.11",
        "@angular/platform-browser": "9.1.11",
        "@angular/platform-browser-dynamic": "9.1.11",
        "@angular/platform-server": "9.1.11",
        "@angular/router": "9.1.11",
        "@nguniversal/builders": "9.1.1",
        "@nguniversal/express-engine": "9.1.1",
        "core-js": "2.5.4",
        "domino": "2.1.5",
        "es5-shim": "4.5.14",
        "es6-shim": "0.35.5",
        "isbot": "3.0.3",
        "rxjs": "6.5.4",
        "tslib": "1.10.0",
        "zone.js": "0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.901.8",
        "@angular/cli": "9.1.8",
        "@angular/compiler-cli": "9.1.11",
        "@angular/language-service": "9.1.11",
        "@types/express-serve-static-core": "4.17.7",
        "@types/node": "12.11.1",
        "@types/jasmine": "3.5.0",
        "@types/jasminewd2": "2.0.3",
        "codelyzer": "5.1.2",
        "husky": "4.2.5",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "5.0.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.0",
        "karma-jasmine": "3.0.1",
        "karma-jasmine-html-reporter": "1.4.2",
        "path": "0.12.7",
        "prettier": "2.0.5",
        "protractor": "7.0.0",
        "ts-node": "8.3.0",
        "tslint": "6.1.0",
        "typescript": "3.8.3"
    }
}
