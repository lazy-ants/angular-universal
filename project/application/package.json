{
    "name": "example-web",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
        "serve:ssr": "npm run silent:update && npm run pm2:stop && npm run pm2:start",
        "pm2:start": "pm2 start dist/server.js --name example.web --max-restarts 5",
        "pm2:stop": "pm2 delete -s example.web || :",
        "build:client-and-server-bundles": "ng run example-web--browser:build:production && ng run example-web--server:build:production",
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
        "silent:update": "rm -rf dist && cp -r tmp dist && rm -rf tmp"
    },
    "private": true,
    "husky": {
        "hooks": {
            "pre-commit": "prettier --write '**/*.ts'"
        }
    },
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/http": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/platform-server": "~7.2.0",
        "@angular/router": "~7.2.0",
        "@gorniv/ngx-transfer-http": "~2.2.9",
        "@nguniversal/express-engine": "7.1.0",
        "@nguniversal/module-map-ngfactory-loader": "7.1.0",
        "angular-l10n": "~7.1.0",
        "classlist.js": "^1.1.20150312",
        "core-js": "^2.5.4",
        "domino": "~2.1.2",
        "es5-shim": "^4.5.12",
        "es6-shim": "^0.35.4",
        "isbot": "^2.2.1",
        "rxjs": "~6.3.3",
        "tslib": "^1.9.0",
        "ts-loader": "^4.4.2",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular/cli": "~7.3.3",
        "@angular/compiler-cli": "^7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/node": "~8.9.4",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "codelyzer": "~4.5.0",
        "husky": "~1.3.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "path": "^0.12.7",
        "prettier": "~1.16.4",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2",
        "webpack-cli": "~3.2.0"
    }
}
