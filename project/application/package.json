{
    "name": "example-web",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "format-ts": "prettier --write \"src/**/*.ts\" --config \".prettierrc-ts.json\"",
        "format-scss": "prettier --write \"src/**/*.scss\" --config \".prettierrc-scss.json\"",
        "format-html": "prettier --write \"src/**/*.html\" --config \".prettierrc-html.json\"",
        "format-json": "prettier --write \"src/**/*.json\" --config \".prettierrc-json.json\"",
        "format": "npm run format-ts && npm run format-scss && npm run format-html && npm run format-json",
        "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
        "serve:ssr": "npm run silent:update && npm run pm2:stop && npm run pm2:start",
        "pm2:start": "pm2 start dist/server.js --name example.web --max-restarts 5",
        "pm2:stop": "pm2 delete -s example.web || :",
        "build:client-and-server-bundles": "ng run example-web--browser:build:production && ng run example-web--server:build:production",
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
        "silent:update": "rm -rf dist && cp -r tmp dist && rm -rf tmp"
    },
    "private": true,
    "husky": {
        "hooks": {
            "pre-commit": "npm run format"
        }
    },
    "dependencies": {
        "@angular/animations": "8.2.14",
        "@angular/common": "8.2.14",
        "@angular/compiler": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/forms": "8.2.14",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/platform-server": "8.2.14",
        "@angular/router": "8.2.14",
        "@gorniv/ngx-transfer-http": "2.2.9",
        "@nguniversal/express-engine": "8.1.1",
        "@nguniversal/module-map-ngfactory-loader": "8.1.1",
        "angular-l10n": "8.1.2",
        "classlist.js": "1.1.20150312",
        "core-js": "2.5.4",
        "domino": "2.1.3",
        "es5-shim": "4.5.13",
        "es6-shim": "0.35.5",
        "isbot": "2.2.1",
        "rxjs": "6.5.3",
        "tslib": "1.10.0",
        "ts-loader": "4.4.2",
        "zone.js": "0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.803.19",
        "@angular/cli": "8.3.19",
        "@angular/compiler-cli": "8.2.14",
        "@angular/language-service": "8.2.14",
        "@types/jasmine": "3.3.8",
        "@types/jasminewd2": "2.0.3",
        "@types/node": "8.9.4",
        "codelyzer": "5.0.1",
        "husky": "3.0.5",
        "jasmine-core": "3.4.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "4.1.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.1",
        "karma-jasmine": "2.0.1",
        "karma-jasmine-html-reporter": "1.4.0",
        "path": "0.12.7",
        "prettier": "1.19.1",
        "protractor": "5.4.0",
        "ts-node": "7.0.0",
        "tslint": "5.15.0",
        "typescript": "3.5.3",
        "webpack-cli": "3.2.0"
    }
}
